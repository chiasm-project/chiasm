{
  "layout": {
    "plugin": "chiasm/plugins/layout",
    "state": {
      "layout": {
        "orientation": "horizontal",
        "children": [
          "editor",
          "scatterPlot"
        ]
      }
    }
  },
  "editor": {
    "plugin": "chiasm/plugins/configEditor",
    "state": {
      "size": "325px"
    }
  },
  "scatterPlot": {
    "plugin": "chiasm/plugins/scatterPlot",
    "state": {
      "xColumn": "petal_width",
      "xAxisLabel": "Petal Width",
      "yColumn": "petal_length",
      "yAxisLabel": "Petal Length",
      "colorColumn": "class",
      "colorDomain": [
        "Iris-setosa",
        "Iris-versicolor",
        "Iris-virginica"
      ],
      "colorRange": [
        "#f66464",
        "#02e837",
        "#00a6f9"
      ],
      "sizeColumn": "petal_length",
      "sizeMin": 4,
      "sizeMax": 14,
      "margin": {
        "top": 32,
        "right": 2,
        "bottom": 40,
        "left": 47
      },
      "xAxisLabelOffset": 1.9,
      "yAxisLabelOffset": 1.4,
      "title": "Iris",
      "titleOffset": -0.269568,
      "sizeDefault": 12
    }
  },
  "barChartCSVLoader": {
    "plugin": "chiasm/plugins/csvLoader",
    "state": {
      "csvPath": "simpleBars.csv",
      "numericColumns": [
        "frequency"
      ]
    }
  },
  "iris": {
    "plugin": "chiasm/plugins/csvLoader",
    "state": {
      "csvPath": "iris.csv",
      "numericColumns": [
        "sepal_length",
        "sepal_width",
        "petal_length",
        "petal_width"
      ]
    }
  },
  "links": {
    "plugin": "chiasm/plugins/links",
    "state": {
      "bindings": [
        "iris.data -> scatterPlot.data"
      ]
    }
  }
}
